UPDATE users
SET id_survey = NULL
WHERE email = 'francesco.memoli@iismargheritahackbaronissi.edu.it';

DELETE FROM users WHERE email = 'francesco.memoli@iismargheritahackbaronissi.edu.it';

SELECT * FROM matches;

